pp/client/src/components/Slider/Slider.js",["101","102"],[],"import { useState, useEffect } from \"react\";\nimport { v4 as uuidv4 } from \"uuid\";\nimport \"./Slider.css\";\n\nexport default function Slider() {\n  const dataSlider = [\n    {\n      src: \"https://wallpaperaccess.com/full/295875.jpg\",\n      id: uuidv4(),\n    },\n    {\n      src: \"https://wallpaperaccess.com/full/295839.jpg\",\n      id: uuidv4(),\n    },\n    {\n      src: \"https://wallpaperaccess.com/full/295881.jpg\",\n      id: uuidv4(),\n    },\n    {\n      src: \"https://wallpaperaccess.com/full/295971.jpg\",\n      id: uuidv4(),\n    },\n    {\n      src: \"https://wallpaperaccess.com/full/295870.jpg\",\n      id: uuidv4(),\n    },\n  ];\n\n  const [slideIndex, setSlideIndex] = useState(1);\n\n  const nextSlide = () => {\n    if (slideIndex !== dataSlider.length) {\n      setSlideIndex(slideIndex + 1);\n    } else if (slideIndex === dataSlider.length) {\n      setSlideIndex(1);\n    }\n  };\n\n  const prevSlide = () => {\n    if (slideIndex !== 1) {\n      setSlideIndex(slideIndex - 1);\n    } else if (slideIndex === 1) {\n      setSlideIndex(dataSlider.length);\n    }\n  };\n\n  const moveDot = (index) => {\n    setSlideIndex(index);\n  };\n\n  useEffect(() => {\n    const timer = setInterval(() => {\n      nextSlide();\n    }, 5000);\n    return () => {\n      clearInterval(timer);\n    };\n  }, [slideIndex]);\n\n  return (\n    <div className=\"container-slider\">\n      <p className=\"main-text\">\n        Enjoy Your Dream <br></br> Vacation In Your <br></br>{\" \"}\n        <div className=\"dream-hotel-text\">DREAM HOTEL</div>\n      </p>\n      {dataSlider.map((obj, index) => {\n        return (\n          <div\n            key={obj.id}\n            className={slideIndex === index + 1 ? \"slide active-anim\" : \"slide\"}\n          >\n            <img src={obj.src} />\n          </div>\n        );\n      })}\n      <div className=\"btn-slide-prev\">\n        <ion-icon\n          name=\"chevron-back-outline\"\n          size=\"large\"\n          onClick={prevSlide}\n        ></ion-icon>\n      </div>\n      <div className=\"btn-slide-next\">\n        <ion-icon\n          name=\"chevron-forward-outline\"\n          size=\"large\"\n          onClick={nextSlide}\n        ></ion-icon>\n      </div>\n      <div className=\"container-dots\">\n        {Array.from({ length: 5 }).map((item, index) => (\n          <div\n            onClick={() => moveDot(index + 1)}\n            className={slideIndex === index + 1 ? \"dot active\" : \"dot\"}\n          ></div>\n        ))}\n      </div>\n    </div>\n  );\n}\n","/Users/Oliwia/bookingapp/client/src/components/PropertyType/PropertyType.js",["103","104","105"],[],"import React from \"react\";\nimport \"./PropertyType.css\";\nimport { v4 as uuidv4 } from \"uuid\";\nimport useFetch from \"../../hooks/useFetch\";\nimport { MoonLoader } from \"react-spinners\";\n\nconst PropertyType = () => {\n  const images = [\n    \"https://img1.10bestmedia.com/Images/Photos/378649/Park-Hyatt-New-York-Manhattan-Sky-Suite-Master-Bedroom-low-res_54_990x660.jpg\",\n    \"https://mir-s3-cdn-cf.behance.net/project_modules/max_1200/dc4d4240971115.5606eec417df7.jpg\",\n    \"https://www.vipholidaybooker.com/storage/app/uploads/public/5da/8f7/f4c/5da8f7f4cb94d724660840.jpg\",\n    \"https://www.sherpareport.com/images/stories/destclubs/Exclusive/er-los-cabos-esperanza-750.jpg\",\n    \"https://hips.hearstapps.com/hmg-prod.s3.amazonaws.com/images/rosewood-mayakoba-residence-1616077172.jpg\",\n    \"https://cdnstep-americantownscom.netdna-ssl.com/img/article/wi-cabin-camping-1.jpg\",\n    \"https://www.tentickle-luxurytents.com/wp-content/uploads/2020/06/76616532_2592366320853490_6985987945891102720_o2.jpg\",\n  ];\n\n  const { data, loading, error } = useFetch(\"/hotels/countByType\");\n\n  return (\n    <div className=\"properties-container\">\n      <p className=\"property-type-text-search\">Browse by property type</p>\n      <div